<?xml version="1.0" encoding="UTF-8"?>

<!DOCTYPE mapper
  PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
  "https://mybatis.org/dtd/mybatis-3-mapper.dtd">
  
<mapper namespace="com.itwillbs.mapper.MemberMapper">

	<!-- 서버시간 정보를 조회 --> 	
 
 	<select id="getServerTime" resultType="string">
 	  select now()
 	</select>

	<!-- 회원가입 --> 
 	<insert id="insertMember">
 		insert into MEMBER (member_idx,member_id,member_name,member_email,member_pw,member_phone
 							member_nick,member_address)
 		values(#{member_idx},#{member_id},#{member_name},#{member_email},#{member_pw},#{member_phone}
 				#{member_nick},#{member_address})
 	
 	</insert> 


</mapper> 